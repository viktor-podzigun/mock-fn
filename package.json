  "engines": {
    "node": ">=16",
    "bun": ">=0.5.8"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "c8": "^7.13.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  }
